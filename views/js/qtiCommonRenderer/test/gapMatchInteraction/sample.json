    "body": {
        "serial": "container_containeritembody_57207342cdc27113603184",
        "body": "\n        <div class=\"grid-row\">\n            <div class=\"col-12\">\n                <h2>Do you know the french poet Charles Baudelaire ?<\/h2>\n            <\/div>\n        <\/div>\n        <div class=\"grid-row\">\n            <div class=\"col-6\">\n                {{interaction_gapmatchinteraction_57207342cf4d4864729612}}\n            <\/div>\n            <div class=\"col-6\">\n                image de Gustave Courbet !\n            <\/div>\n        <\/div>\n    ",
        "elements": {
            "interaction_gapmatchinteraction_57207342cf4d4864729612": {
                "serial": "interaction_gapmatchinteraction_57207342cf4d4864729612",
